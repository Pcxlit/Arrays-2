#include<iostream>
using namespace std;
int main (){
cout<<"enter number of elements in an array"<<endl;   //create an array
    int n;
    cin>>n;
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        cout<<"enter value of array index "<<i<<endl;
        cin>>arr[i];
    }
    
    for (int i = 0; i < n; i++)     //display an array
    {
        cout<<arr[i]<<" ";
    }
    int distinct_elements=n;
    for (int i = 0; i < n; i++)
    {
        for (int j= (i+1); j < n; j++)
        {
         if (i==j)
        {
            distinct_elements--;
        }
        }
    }
    cout<<"total distinct elemnets are "<<distinct_elements;
    return 0;
} 
