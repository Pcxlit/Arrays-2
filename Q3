#include <iostream>
using namespace std;

int main() {
    int a[100], n, sum = 0, full;

    cout << "Enter number of elements (one missing): ";
    cin >> n;

    cout << "Enter elements:\n";
    for (int i = 0; i < n; i++) {
        cin >> a[i];
        sum += a[i];
    }

    full = (n + 1) * (n + 2) / 2;

    cout << "Missing number: " << full - sum << endl;

    return 0;
}
